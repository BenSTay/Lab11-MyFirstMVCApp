@{
    ViewData["Title"] = "Home";
}
<div>
    <h1>TIME</h1>
    <h2>Person of the Year</h2>
    <form asp-controller="Home" asp-action="Search" method="get">
        <label for="start">Start Year</label>
        <input name="start" type="number" min="1927" max="2016" required />
        <label for="end">End Year</label>
        <input name="end" type="number" min="1927" max="2016" required />
        <input type="submit" value="Search" />
    </form>
</div>


